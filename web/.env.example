# Frontend Environment Variables
# Copy this file to .env and fill in your values

# API Key for authentication
# This key is sent with every API request in the X-API-Key header
VITE_API_KEY=your-api-key-here

# API Base URL (optional)
# Leave empty for same-origin requests, or set to full URL if API is on different host
# Examples:
#   VITE_API_BASE_URL=             (same origin - recommended for production)
#   VITE_API_BASE_URL=http://localhost:8080  (for development with separate backend)
VITE_API_BASE_URL=
